{
  "name": "@githubnext/vscode-react-webviews",
  "version": "1.0.0",
  "description": "A sample/starter template for developing VS Code extensions with webviews",
  "main": "out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "format": "prettier --write **/*.ts",
    "dev": "vite build"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/githubnext/vscode-react-webviews.git"
  },
  "keywords": [
    "vscode"
  ],
  "author": "Next Devex <next-devex@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/githubnext/vscode-react-webviews.git/issues"
  },
  "homepage": "https://github.com/githubnext/vscode-react-webviews.git#readme",
  "devDependencies": {
    "@githubocto/tailwind-vscode": "^1.0.4",
    "@types/node": "^16.4.13",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/tailwindcss": "^2.2.1",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.1.0",
    "postcss": "^8.5.6",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hediet/node-reload": "^0.7.3",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "history": "^5.0.1",
    "lucide-react": "^0.563.0",
    "nanoid": "^3.1.23",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^4.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.3",
    "react-shiki": "^0.9.1",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.4.21",
    "zustand": "^5.0.10"
  },
  "activationEvents": [
    "onCommand:NextWebview1.start",
    "onCommand:NextWebview2.start"
  ],
  "contributes": {
    "commands": [
      {
        "command": "NextWebview1.start",
        "title": "Open AI Chat",
        "category": "Movesia"
      },
      {
        "command": "NextWebview2.start",
        "title": "Open Webview 2",
        "category": "Movesia"
      }
    ]
  }
}
