{
  "name": "movesia-extension",
  "publisher": "movesia",
  "version": "1.0.0",
  "description": "Movesia AI â€” Unity development assistant",
  "main": "out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node --max-old-space-size=8192 node_modules/typescript/bin/tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "format": "prettier --write **/*.ts",
    "dev": "vite build"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/githubnext/vscode-react-webviews.git"
  },
  "keywords": [
    "vscode"
  ],
  "author": "Next Devex <next-devex@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/githubnext/vscode-react-webviews.git/issues"
  },
  "homepage": "https://github.com/githubnext/vscode-react-webviews.git#readme",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^20.19.32",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.59.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^8.1.0",
    "prettier": "^2.3.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hediet/node-reload": "^0.7.3",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.1.15",
    "@langchain/langgraph": "^1.1.3",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@langchain/tavily": "^1.2.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "history": "^5.0.1",
    "lucide-react": "^0.563.0",
    "nanoid": "^3.1.23",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^4.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.3",
    "react-shiki": "^0.9.1",
    "remark-gfm": "^4.0.1",
    "sql.js": "^1.13.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "vite": "^5.4.21",
    "ws": "^8.18.0",
    "zod": "3.25.67",
    "zustand": "^5.0.10"
  },
  "overrides": {
    "zod": "3.25.67",
    "@langchain/core": "^1.1.15"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "NextWebview1.start",
        "title": "Open AI Chat",
        "category": "Movesia"
      },
      {
        "command": "NextWebview2.start",
        "title": "Open Webview 2",
        "category": "Movesia"
      },
      {
        "command": "movesia.installUnityPackage",
        "title": "Install Unity Package",
        "category": "Movesia"
      },
      {
        "command": "movesia.uninstallUnityPackage",
        "title": "Uninstall Unity Package",
        "category": "Movesia"
      },
      {
        "command": "movesia.selectUnityProject",
        "title": "Select Unity Project",
        "category": "Movesia"
      },
      {
        "command": "movesia.signIn",
        "title": "Sign In",
        "category": "Movesia"
      },
      {
        "command": "movesia.signOut",
        "title": "Sign Out",
        "category": "Movesia"
      }
    ]
  }
}
